{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint . --cache --quiet",
    "lint": "yarn lint:js",
    "lintfix": "yarn lint:js --fix",
    "lint:translation": "npx @inlang/cli validate --project ./project.inlang && npx @inlang/cli lint --project ./project.inlang",
    "eslint-viewer": "npx @eslint/config-inspector",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=$(grep CHROMATIC_KEY .env | cut -d '=' -f2)",
    "prepare-husky": "cd ../.. && husky app/.husky",
    "postinstall": "yarn prepare-husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.9.0",
    "@next/bundle-analyzer": "^14.2.5",
    "accept-language": "^3.0.18",
    "framer-motion": "^11.3.24",
    "i18next": "^23.12.2",
    "i18next-resources-to-backend": "^1.2.1",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.2",
    "react": "^18.3.1",
    "react-cookie": "^7.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-i18next": "^15.0.1",
    "tailwind-merge": "^2.4.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@chromatic-com/storybook": "^1.6.1",
    "@eslint-react/eslint-plugin": "^1.9.1",
    "@eslint/eslintrc": "^3.1.0",
    "@next/eslint-plugin-next": "^14.2.5",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-onboarding": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/nextjs": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@stylistic/eslint-plugin": "^2.6.1",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "chromatic": "^11.7.0",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.4",
    "cssnano-preset-advanced": "^7.0.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.0.0",
    "sass": "^1.77.8",
    "storybook": "^8.2.8",
    "tailwindcss": "^3.4.9",
    "tsc-files": "^1.1.4",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@4.3.1",
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc-files --noEmit --project tsconfig.json"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
