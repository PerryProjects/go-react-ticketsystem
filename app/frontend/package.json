{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build --turbo",
    "start": "next start",
    "lint:js": "eslint . --cache",
    "lint": "yarn lint:js",
    "lintfix": "yarn lint:js --fix",
    "clean": "rimraf .next",
    "nuke:install": "rimraf ./node_modules ./package-lock.json",
    "toc": "i18next-resources-for-ts toc -i ./public/locales/",
    "merge": "i18next-resources-for-ts merge -i ./public/locales/en -o ./@types/resources.json",
    "interface": "i18next-resources-for-ts interface -i ./public/locales/en -o ./@types/resources.d.ts"
  },
  "dependencies": {
    "i18next": "^23.11.2",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "next-i18next": "^15.2.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@eslint/eslintrc": "^3.0.2",
    "@next/eslint-plugin-next": "^14.2.1",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "i18next-resources-for-ts": "1.5.0",
    "picocolors": "^1.0.0",
    "primeflex": "^3.3.1",
    "rimraf": "^5.0.5",
    "sass": "^1.75.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.1.1",
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ]
}
